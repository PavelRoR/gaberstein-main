#video
	padding 90px 0 30px
	@media screen and (max-width 991px)
		padding 60px 0 0
.video-list
	// background: radial-gradient(50% 50% at 50% 50%,#fff 0%,#f4f4f4 100%);
	border-radius: 10px;
	padding: 0 30px
	margin-top: 55px
	@media screen and (max-width 1199px)
		margin-top: 25px
		padding 0
	li
		list-style: none
		position relative
		margin-bottom: 20px
		padding-left: 20px
		@media screen and (max-width 560px)
			margin-bottom: 10px
		&:before
			content: ''
			display block
			position absolute
			top 11px
			left 0
			border-radius: 50%
			width 9px
			height: 9px
			background-color: #E8CEF5